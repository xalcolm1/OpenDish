<!DOCTYPE html>
<html>
  <head>
    <title>TableOuverte</title>
  
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= favicon_link_tag asset_path('ouverte-fav.png') %>
    <%= stylesheet_link_tag 'application', 'https://fonts.googleapis.com/css2?family=Commissioner:wght@500&display=swap',
        media: 'all', 'data-turbolinks-track' => true %>
  </head>

  <body>
    <%= yield %>
      <script type="text/javascript">
            window.OuverteURL = "<%= image_url('Ouverte-main.png') %>";

       
            window.TableURL = "<%= image_url('table.jpeg') %>";
      <% if current_user %>
        
          window.currentUser = {'id': '<%=current_user.id%>'}
      <% end %>
        </script>

    <footer>
      <div class="resources">
        <p> 
        scources: <br/>
        Created logo at LogoMakr.com <br/>
        image by Nils Stahl
        </p>
        <p> 
        external links:
        </p>
      </div>
    </footer>
  </body>

</html>   
